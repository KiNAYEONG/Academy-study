   "source": [
    "pre = int(input())\n",
    "score = list(map(int, input().split()))\n",
    "score.sort()\n",
    "new_score = []    \n",
    "max_score = score[-1]\n",
    "\n",
    "for i in score:\n",
    "  i = (i/max_score)*100\n",
    "  new_score.append(i)\n",
    "avg = sum(new_score)/len(new_score)\n",
    "print(avg)"
   ]
  }
